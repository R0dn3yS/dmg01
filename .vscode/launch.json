{
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/src/build/gbemu/gbemu",
      "args": ["roms/mem_timing.gb"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "version": "2.0.0"
}